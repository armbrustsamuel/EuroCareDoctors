<mvc:View 
	controllerName="EuroCareDoctor.controller.ChatBotAnswer"
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Eurocare Chatbot Answer"
				showNavButton="true"
				navButtonPress="handleNavButtonPress">
				<content>
					<Panel>
						<Label text="Product" labelFor="productInput"/>
						<TextArea value="" growing="true" width="100%"/>
					</Panel>
				</content>
				<footer>
				    <OverflowToolbar>
				    	<!--<core:Icon-->
				    	<!--	src="sap-icon://user-settings"/>-->
				        <ToolbarSpacer/>
				        <Image
            		    	visible="true"
            		    	width="80px"
            		    	height="40px"
            		    	src="/webapp/img/SAP_2011_logo.svg"/>
				    </OverflowToolbar>
        		</footer>
			</Page>
		</pages>
	</App>
</mvc:View>